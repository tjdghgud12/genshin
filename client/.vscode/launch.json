{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next Server",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn dev",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/.pnp.cjs", "${workspaceFolder}/.yarn/**"],
      "sourceMaps": true
    },
    {
      "name": "launch to Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true
    }
  ],
  "compounds": [
    {
      "name": "Next Process",
      "configurations": ["Next Server", "launch to Chrome"]
    }
  ]
}
